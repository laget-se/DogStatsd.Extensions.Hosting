<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>DogStatsd.Extensions.Hosting</id>
    <title>DogStatsD.Extensions.Hosting</title>
    <version>$version$</version>
    <authors>laget.se</authors>
    <owners>laget.se</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Hosting and startup abstractions for DogStatsD. When using NuGet 3.x this package requires at least version 3.4....</description>
    <summary></summary>
    <license type="expression">Apache-2.0</license>
    <projectUrl>https://github.com/laget-se/DogStatsd.Extensions.Hosting</projectUrl>
    <icon>assets\icon.png</icon>
    <readme>assets\README.md</readme>
    <repository type="git" url="https://github.com/laget-se/DogStatsd.Extensions.Hosting" branch="master" />
    <dependencies>
      <group targetFramework="net8.0">
        <dependency id="DogStatsD-CSharp-Client" version="8.0.0" />
        <dependency id="Microsoft.Extensions.Hosting.Abstractions" version="8.0.0" />
      </group>
      <group targetFramework="net6.0">
        <dependency id="DogStatsD-CSharp-Client" version="8.0.0" />
        <dependency id="Microsoft.Extensions.Hosting.Abstractions" version="8.0.0" />
      </group>
      <group targetFramework="netstandard2.1">
        <dependency id="DogStatsD-CSharp-Client" version="8.0.0" />
        <dependency id="Microsoft.Extensions.Hosting.Abstractions" version="8.0.0" />
      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="DogStatsD-CSharp-Client" version="8.0.0" />
        <dependency id="Microsoft.Extensions.Hosting.Abstractions" version="8.0.0" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\README.md" target="assets\" />
    <file src="..\assets\icon.png" target="assets\" />
    <file src=".\bin\Release\net8.0\*" target="lib\net8.0\" />
    <file src=".\bin\Release\net6.0\*" target="lib\net6.0\" />
    <file src=".\bin\Release\netstandard2.1\*" target="lib\netstandard2.1\" />
    <file src=".\bin\Release\netstandard2.0\*" target="lib\netstandard2.0\" />
  </files>
</package>
